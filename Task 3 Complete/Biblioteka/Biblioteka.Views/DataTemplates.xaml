<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:Biblioteka.Views"
                    xmlns:viewmodels="clr-namespace:Biblioteka.ViewModels;assembly=Biblioteka.ViewModels">

    <DataTemplate DataType="{x:Type viewmodels:BooksViewModel}">
        <views:BooksView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:AddBookViewModel}">
        <views:AddBookView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:EditBookViewModel}">
        <views:EditBookView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:SearchViewModel}">
        <views:SearchView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:UsersViewModel}">
        <views:UsersView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:AddUserViewModel}">
        <views:AddUserView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:EditUserViewModel}">
        <views:EditUserView />
    </DataTemplate>


</ResourceDictionary>